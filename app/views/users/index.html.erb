<h1>Users</h1>
<table>
    <thread>
        <tr>
            <th>image</th>
            <th>name</th>
        </tr>
    </thread>
    <tbody>
        <% @users.each do |user| %>
            <tr>
            <td>
                <%= attachment_image_tag @user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg"%>
            </td>
            <td>
                <%= user.name %>
            </td>
            <td>
            </td>
            </tr>
        <% end %>
        </tbody>
</table>        
